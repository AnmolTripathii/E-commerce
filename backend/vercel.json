{
    "version": 2,
    "builds": [
      { "src": "*.js", "use": "@vercel/node" }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/$1"
      },
      {
        "src": "/popularinwomen",
        "methods": ["GET", "OPTIONS"],
        "headers": {
          "Access-Control-Allow-Origin": "https://e-commerce-dx27.vercel.app"
        },
        "dest": "https://e-commerce-virid-phi.vercel.app/popularinwomen"
      },
      {
        "src": "/newcollections",
        "methods": ["GET", "OPTIONS"],
        "headers": {
          "Access-Control-Allow-Origin": "https://e-commerce-dx27.vercel.app"
        },
        "dest": "https://e-commerce-virid-phi.vercel.app/newcollections"
      },
      {
        "src": "/allproducts",
        "methods": ["GET", "OPTIONS"],
        "headers": {
          "Access-Control-Allow-Origin": "https://e-commerce-dx27.vercel.app"
        },
        "dest": "https://e-commerce-virid-phi.vercel.app/allproducts"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  